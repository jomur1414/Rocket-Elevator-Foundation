<p id="notice"><%= notice %></p>

<h1>Geolocations</h1>

<table>
  <thead>
    <tr>
      <th>Click below to access Locations</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @geolocations.each do |geolocation| %>
      <tr>

        <td><%= link_to 'Show Locations', geolocation %></td>

      </tr>
    <% end %>
  </tbody>
</table>

